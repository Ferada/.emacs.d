Xft.dpi: 96.0
Xft.rgba: rgb
Xft.antialias: true
Xft.hinting: true

xpdf*viKeys: true

!!Emacs.font: -*-fixed-*-*-*-*-*-*-*-*-*-*-iso10646-*
!Emacs.font: 6x12
Emacs.font: Bitstream Vera Sans Mono-7.5
Emacs.fontBackend: xft
Emacs.menuBar: false
Emacs.toolBar: false
Emacs.background: #020202
Emacs.foreground: #b7af9f
Emacs.cursorColor: #d0d0df

!! urxvt
URxvt*loginShell: false
URxvt*font: xft:Bitstream Vera Sans Mono:pixelsize=11
URxvt*saveLines: 500
URxvt*geometry: 80x25
URxvt*scrollBar: false
URxvt*keysym*Home: \033[1~
URxvt*keysym*End: \033[4~
!URxvt*keysym*Insert: \033[2~

!! Greenish theme
URxvtGreen*color0: #000000
URxvtGreen*color8: #666666
!! DarkRed + Red
URxvtGreen*color1: #b45151
URxvtGreen*color9: #fe8686
!! DarkGreen + Green
URxvtGreen*color2: #8ba574
URxvtGreen*color10: #a5dca5
!! DarkYellow + Yellow
URxvtGreen*color3: Khaki3
URxvtGreen*color11: #efe58b
!! DarkBlue + Blue
URxvtGreen*color4: #2797d8
URxvtGreen*color12: #86cdea
!! DarkMangenta + Mangenta
URxvtGreen*color5: #aa6eaf
URxvtGreen*color13: #b698b8
!!DarkCyan + Cyan
URxvtGreen*color6: #418179
URxvtGreen*color14: #71bebe
!! LightGrey + White
URxvtGreen*color7: #bebebe
URxvtGreen*color15: #ffffff

!! urxvt orange
URxvtOrange*cursorColor:   #ff7020
!URxvtOrange*cursorColor2:  #000000 ! which defaults to background
URxvtOrange*background:    black
URxvtOrange*foreground:    #ff9040
! black, bright black
URxvtOrange*color0:        black
URxvtOrange*color8:        #303030
! red, bright red
URxvtOrange*color1:        #dc1616
URxvtOrange*color9:        #fc3c2c
! green, bright green
URxvtOrange*color2:        #73c400
URxvtOrange*color10:       #94de00
! yellow, bright yellow
URxvtOrange*color3:        #e69109
URxvtOrange*color11:       #ffc319
! blue, bright blue
URxvtOrange*color4:        #403cb1
URxvtOrange*color12:       #00749e
! purple, bright purple
URxvtOrange*color5:        #8326bf
URxvtOrange*color13:       #a24cbf
! cyan, bright cyan
