Xft.dpi: 96.0
Xft.rgba: rgb
Xft.antialias: true
Xft.hinting: true

xpdf*viKeys: true

!!Emacs.font: -*-fixed-*-*-*-*-*-*-*-*-*-*-iso10646-*
!Emacs.font: 6x12
Emacs.font: Bitstream Vera Sans Mono-7.5
Emacs.fontBackend: xft
Emacs.menuBar: false
Emacs.toolBar: false
Emacs.background: #020202
Emacs.foreground: #b7af9f
Emacs.cursorColor: #d0d0df

!! urxvt
URxvt*loginShell: false
URxvt*font: xft:Monospace:pixelsize=10
URxvt*saveLines: 500
URxvt*geometry: 80x25
URxvt*scrollBar: false
URxvt*keysym*Home: \033[1~
URxvt*keysym*End: \033[4~
!URxvt*keysym*Insert: \033[2~

!! Greenish theme
URxvtGreen*color0: #000000
URxvtGreen*color8: #666666
!! DarkRed + Red
URxvtGreen*color1: #b45151
URxvtGreen*color9: #fe8686
!! DarkGreen + Green
URxvtGreen*color2: #8ba574
URxvtGreen*color10: #a5dca5
!! DarkYellow + Yellow
URxvtGreen*color3: Khaki3
URxvtGreen*color11: #efe58b
!! DarkBlue + Blue
URxvtGreen*color4: #2797d8
URxvtGreen*color12: #86cdea
!! DarkMangenta + Mangenta
URxvtGreen*color5: #aa6eaf
URxvtGreen*color13: #b698b8
!!DarkCyan + Cyan
URxvtGreen*color6: #418179
URxvtGreen*color14: #71bebe
!! LightGrey + White
URxvtGreen*color7: #bebebe
URxvtGreen*color15: #ffffff

!! urxvt orange
URxvt*cursorColor:   #ff7020
!URxvt*cursorColor2:  #000000 ! which defaults to background
URxvt*background:    black
URxvt*foreground:    #ff9040
! black, bright black
URxvt*color0:        black
URxvt*color8:        #303030
! red, bright red
URxvt*color1:        #dc1616
URxvt*color9:        #fc3c2c
! green, bright green
URxvt*color2:        #73c400
URxvt*color10:       #94de00
! yellow, bright yellow
URxvt*color3:        #e69109
URxvt*color11:       #ffc319
! blue, bright blue
URxvt*color4:        #403cb1
URxvt*color12:       #00749e
! purple, bright purple
URxvt*color5:        #8326bf
URxvt*color13:       #a24cbf
! cyan, bright cyan
URxvt*color6:        #217796
URxvt*color14:       #4c97bf
! white, bright white
URxvt*color7:        #ffdb94
URxvt*color15:       #fff3db

!! urxvt blueish, merged with emacs color theme
URxvtBlue*cursorColor: #d0d0df
